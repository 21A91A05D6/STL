#include <iostream>
using namespace std;
#include<vector>
int main() {
   vector<int>a;
   
   a.push_back(99);
   a.push_back(999);
   a.push_back(9999);
   
   auto x=a.begin();
   auto y=a.end()-1;
   auto x1=a.rbegin();
   auto y1=a.rend()-1;
   
   cout<<*x<<" "<<*y<<endl;
   cout<<*x1<<" "<<*y1;
   
   return 0;
   
   //99 9999
   //9999 99
}
